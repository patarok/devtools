#!/bin/sh

for setup in `detect-dev-setup`; do
    if [ "$setup" == "docker-compose" ]; then
        echo "running command: docker-compose ps -a -q | xargs -I {} docker inspect --format '{{.Id}} {{.Name}} {{.State.Status}}' {}"

        uid=$(id -u) gid=$(id -g) docker-compose ps -a -q | xargs -I {} docker inspect --format '{{.Id}} {{.Name}} {{.State.Status}}' {}
    fi
done

